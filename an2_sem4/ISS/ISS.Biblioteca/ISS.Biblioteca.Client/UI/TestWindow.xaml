<Window x:Class="ISS.Biblioteca.Commons.UI.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISS.Biblioteca.Commons.UI"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Margin="5" Name="LoginButton" Click="LoginButton_Click">Login Window</Button>
        <Button Margin="5" Name="AbonatButton" Click="AbonatButton_Click">AbonatWindow</Button>
        <Button Margin="5" Name="BibliotecarButton" Click="BibliotecarButton_Click">BibliotecarWindow</Button>
        <Button Margin="5" Name="AdminButton" Click="AdminButton_Click">AdminWindow</Button>
    </StackPanel>
</Window>
